function bxgy_format_number (num) {
  var fraction = num - parseInt(num);
  fraction = fraction.toFixed(2);
  if (num == fraction) {return fraction.toString(); }

  var str = fraction.toString();
  str = str.replace("0.",".");
  var n = parseInt(num);
  var i = 0;
  while (n > 0) {
    var d = n % 10;
    n = parseInt(n / 10);
    str = d.toString() + str;
    i++;
    if ((i % 3 == 0) && (n > 0)) { str = "," + str; }
  }
  return str;
}

