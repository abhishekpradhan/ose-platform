	position: absolute;
	width: 34px;
	height: 100%;
	left: 0;
	top: 0;
	bottom: 0;
	overflow: hidden;
}
.poBackground .poBody .poMiddle {
	position: relative;
	background-color: #FFFFFF;
	height: 100%;
	margin: 0 34px;
}
.poBackground .poBody .poRight {
	position: absolute;
	width: 34px;
	height: 100%;
	right: 0;
	top: 0;
	bottom: 0;
