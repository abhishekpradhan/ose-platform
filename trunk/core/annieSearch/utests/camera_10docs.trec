    listPriceValueStr = bxgy_format_number(listPriceValue);
    listPrice.innerHTML = listPrice.innerHTML.replace(/([\d\.,]+)/,listPriceValueStr);
    listPriceT.innerHTML = bxgyLPTexts[isXChecked+isYChecked+isZChecked];
  }

  var buyingPriceValue = buyingPrices[0]*isXChecked + buyingPrices[1]*isYChecked + buyingPrices[2]*isZChecked;
