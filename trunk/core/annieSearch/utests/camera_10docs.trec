function fixIEPngs(matcheeID, matcherID) {
    var arVersion = navigator.appVersion.split("MSIE");
    var version = parseFloat(arVersion[1]);

   if ((version >= 5.5 && version < 8) && (document.body.filters))
   {
      var matchee = get(matcheeID);
      var matcheeHeight = matchee.offsetHeight;
      var matcheeWidth = matchee.offsetWidth;

      var matcher = get(matcherID); 
      var spans = matcher.getElementsByTagName('span');

      for(var i = 0; i < spans.length; i++)
      {
        if ( spans[i].className.indexOf('tafPngSpan') != -1 )
        {
            if ( spans[i].style.width != '34px' )
            {
                spans[i].style.width = matcheeWidth - 68;
            } else if ( spans[i].style.height != '34px' ) {
                spans[i].style.height = matcheeHeight - 68;
            }
        }
      }


      var imgs = matcher.getElementsByTagName('img');
      for(var i=0; i<imgs.length; i++)
      {
         var img = imgs[i];
         var imgName = img.src.toUpperCase();
         if (imgName.substring(imgName.length-3, imgName.length) == "PNG")
         {
            var imgID = (img.id) ? "id='" + img.id + "' " : "";
            var imgClass = "class='tafPngSpan" + ( (img.className) ? " " + img.className : "" ) + "' ";
            var imgTitle = (img.title) ? "title='" + img.title + "' " : "title='" + img.alt + "' ";
            var imgStyle = img.style.cssText;
            if (img.align == "left") imgStyle = "float:left;" + imgStyle;
            if (img.align == "right") imgStyle = "float:right;" + imgStyle;
            if (img.parentElement.href) imgStyle = "cursor:hand;" + imgStyle;
            var strNewHTML = "<span " + imgID + imgClass + imgTitle
            + " style=\"" + "width:" + img.width + "px; height:" + img.height + "px;" + imgStyle + ";"
            + "filter:progid:DXImageTransform.Microsoft.AlphaImageLoader"
            + "(src=\'" + img.src + "\', sizingMethod='scale');\"><img src='http://g-ecx.images-amazon.com/images/G/01/x-locale/common/spacer._V47082042_.gif' height='1' width='1'></span>";

            img.outerHTML = strNewHTML;

            i = i-1;
         }
      }
   }
}

function get(id){
    return document.getElementById(id);
}

function fixSafariPngs(matcheeID, matcherID) {
    if ( navigator.userAgent.toUpperCase().indexOf("SAFARI") == -1 )
      return;

    var matcheeHeight = get(matcheeID).offsetHeight;
    var matcher = get(matcherID); 
    var images = matcher.getElementsByTagName("img");

    for(var i=0; i<images.length; i++)
