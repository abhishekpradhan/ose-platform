	function getRequestInstance()

	{

		var req_instance = null;

		if (window.XMLHttpRequest)

		{

			req_instance = new XMLHttpRequest();

		}

		else

		{

			if(window.ActiveXObject)

			{

				try

				{

					req_instance = new ActiveXObject("Msxml2.XMLHTTP");

				}

				catch(e){}

				if(!req_instance)

				{

					try

					{

						req_instance = new ActiveXObject("Microsoft.XMLHTTP");

					}

					catch(e){}

				}

			}

		}

		return req_instance;

	}



	function suggestRequest_changed()	

	{

		if (suggestRequester.readyState == 4)

		{

			if (suggestRequester.status == 200)

			{

				if(suggestRequester.responseText=="")

					autocomplete.style.visibility = "hidden";

				else

				{

					autocomplete.innerHTML = suggestRequester.responseText;

					autocomplete.style.visibility = "visible";

				}

			}

			else

			{

				autocomplete.style.visibility = "hidden";

				autocomplete.innerHTML = "";

			}

		}

	}



	function SendQuery(e)

	{

		if (!e) e = window.event;

		switch(e.keyCode)

		{

			case 13://non-printable keys to ignore

			case 16:

			case 17:

			case 20:

			case 37:

			case 39:

				break;

			case 27://escape key

				autocomplete.style.visibility = "hidden";

				break;

			case 40:// down arrow

				selectItemDown();

				break;

			case 38://up arrow

				selectItemUp();

				break;

			default:

				if(queryBox.lastValue == queryBox.value)

					return true;



				queryBox.lastValue = queryBox.value;



				if(queryBox.value.length < 3)

				{

					if(suggestRequester && suggestRequester.readyState != 0)

						suggestRequester.abort();

					autocomplete.style.visibility = "hidden";

					return true;

				}



				autocomplete.activeIndex = -1;

				var url = "/retail/autocomplete.asp?q=" + escape(queryBox.value) + "&s=" + queryTypeToStoreID(storeDropDown.value);

				if(suggestRequester)

				{

					suggestRequester.open("GET", url, true);

					suggestRequester.onreadystatechange = suggestRequest_changed;

					suggestRequester.send(null);

				}

				break;

		}

		return true;

	}

	

	function selectItemDown()

	{

		if(autocomplete.style.visibility == "hidden")

			return;



		if(autocomplete.activeIndex < autocomplete.childNodes.length-1)

			autocomplete.activeIndex++;

		else

			autocomplete.activeIndex = autocomplete.childNodes.length-1;



		if(autocomplete.activeIndex>0)

			autocomplete.childNodes[autocomplete.activeIndex-1].className = "";

		autocomplete.childNodes[autocomplete.activeIndex].className = "activeLink";



		queryBox.value = autocomplete.childNodes[autocomplete.activeIndex].firstChild.innerHTML;



		//attempt to set selection range

		if(queryBox.lastValue)

		{

			if(queryBox.createTextRange)

			{

				var selection = queryBox.createTextRange();

				selection.moveStart("character", queryBox.lastValue.length);

				selection.moveEnd("character", queryBox.value.length-1);

				selection.select();

			}

			else if(queryBox.setSelectionRange)

			{

				queryBox.setSelectionRange(queryBox.lastValue.length,queryBox.value.length);

			}

		}

	}



	function selectItemUp()

	{

		if(autocomplete.style.visibility == "hidden")

			return;



		if(autocomplete.activeIndex > autocomplete.childNodes.length)

			autocomplete.activeIndex = autocomplete.childNodes.length;

		if(autocomplete.activeIndex < 1)//

		{

			autocomplete.activeIndex = -1;

			queryBox.value = queryBox.lastValue;//value.substring(0,queryBox.lastValue.length);

			autocomplete.childNodes[0].className = "";

			return;

		}



		autocomplete.activeIndex--;

		if(autocomplete.activeIndex < autocomplete.childNodes.length-1)

			autocomplete.childNodes[autocomplete.activeIndex+1].className = "";

		autocomplete.childNodes[autocomplete.activeIndex].className = "activeLink";



		queryBox.value = autocomplete.childNodes[autocomplete.activeIndex].firstChild.innerHTML;

		

		//attempt to set selection range

		if(queryBox.lastValue)

		{

			if(queryBox.createTextRange)

			{

				var selection = queryBox.createTextRange();

				selection.moveStart("character", queryBox.lastValue.length);

				selection.moveEnd("character", queryBox.length-1);

				selection.select();

			}

			else if(queryBox.setSelectionRange)

			{

				queryBox.setSelectionRange(queryBox.lastValue.length,queryBox.value.length);

			}

		}

	}



	function go(query)

	{

		theform.suggest.value = "1";

		queryBox.value = query;

		autocomplete.style.visibility = "hidden";

		theform.submit();

	}

	

	function searchbox_submit()

	{

		if(autocomplete.style.visibility != "hidden")

		{

			if(autocomplete.activeIndex > -1 && autocomplete.activeIndex < autocomplete.childNodes.length)

			{

				if(queryBox.value == autocomplete.childNodes[autocomplete.activeIndex].firstChild.innerHTML)

				{

					theform.suggest.value = "1";

				}

			}

		}

		return true;

	}

